{
  "name": "next2nothing",
  "version": "0.1.0",
  "description": "Another opinionated next.js typescript boilerplate",
  "private": true,
  "license": "BSD-3",
  "homepage": "",
  "author": "einSelbst",
  "keywords": [
    "next.js",
    "wundergraph",
    "turbo",
    "typescript"
  ],
  "repository": "https://github.com/einSelbst/next2nothing",
  "bugs": "https://github.com/einSelbst/next2nothing/issues",
  "scripts": {
    "prepare": "is-ci || husky install",
    "preinstall": "is-ci || npx only-allow pnpm",
    "xpostinstall": "is-ci || typesync",
    " ----------------------- Development ----------------------- ": "",
    "build": "turbo run build",
    "start": "pnpm start:turbo & pnpm open",
    "start:turbo": "turbo run start",
    "dev": "turbo run dev --parallel",
    "generate": "turbo run generate",
    "clean": "turbo run clean",
    "watch": "echo \"Error: no command specified\" && exit 1",
    " ------------------------- Testing ------------------------- ": "",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cover": "echo \"Error: no test specified\" && exit 1",
    " ------------------------- Linting ------------------------- ": "",
    "?format": "echo Format source code and fix issues",
    "format": "prettier --check --write --ignore-unknown . --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-sh --plugin=prettier-plugin-toml",
    "?lint": "echo Lint source code",
    "lint": "turbo run lint --parallel",
    "-------------------------- Docker ---------------------------": "",
    "docker:build": "docker build -t n2n:latest .",
    "docker:run": "docker container run -d -p 3000:80 --name n2n n2n:latest",
    "docker:refresh": "docker compose --verbose up -d --build",
    "dock": "COMPOSE_DOCKER_CLI_BUILD=1 docker-compose -f docker-compose.yml build --parallel",
    "docke": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose -f docker-compose.yml build --parallel",
    "docki": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=0 docker compose up -d --build",
    " ------------------------- Helpers ------------------------- ": "",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:3000",
    "?": "npm-scripts-info"
  },
  "dependencies": {
    "is-ci": "3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@types/node": "18.11.18",
    "eslint": "8.32.0",
    "eslint-config-custom": "0.0.0",
    "git-format-staged": "3.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "npm-scripts-info": "0.3.9",
    "open-cli": "7.1.0",
    "prettier": "2.8.3",
    "prettier-plugin-sh": "0.12.8",
    "prettier-plugin-tailwindcss": "0.2.1",
    "prettier-plugin-toml": "0.3.1",
    "turbo": "latest",
    "typescript": "4.9.4",
    "wait-on": "7.0.1"
  },
  "packageManager": "pnpm@7.20.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  }
}
